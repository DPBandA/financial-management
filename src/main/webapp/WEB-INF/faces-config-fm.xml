<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <converter>
        <converter-id>activeSupplierConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ActiveSupplierConverter</converter-class>
    </converter>
    <converter>
        <converter-id>classificationConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ClassificationConverter</converter-class>
    </converter>
    <converter>
        <converter-id>jobCategoryConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.JobCategoryConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>jobSubCategoryConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.JobSubCategoryConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>sectorConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.SectorConverter</converter-class>
    </converter>    
    <converter>
        <converter-id>activeInventoryProductConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ActiveInventoryProductConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeInventoryConverter</converter-id>
        <converter-class>jm.com.dpbennett.fm.converter.ActiveInventoryConverter</converter-class>
    </converter>   
       
    <validator>
        <validator-id>supplierValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.SupplierValidator</validator-class>
    </validator>
    <validator>
        <validator-id>purchaseReqNumberValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.PurchaseReqNumberValidator</validator-class>
    </validator>
    <validator>
        <validator-id>purchaseOrderNumberValidator</validator-id>
        <validator-class>jm.com.dpbennett.fm.validator.PurchaseOrderNumberValidator</validator-class>
    </validator>    
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>purchreqDialog</from-outcome>
            <to-view-id>/finance/purch/purchreqDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventoryDialog</from-outcome>
            <to-view-id>/finance/ims/inventoryDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventoryRequisitionDialog</from-outcome>
            <to-view-id>/finance/ims/inventoryRequisitionDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>purchaseReqEmailDialog</from-outcome>
            <to-view-id>/finance/purch/purchaseReqEmailDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>supplierDialog</from-outcome>
            <to-view-id>/finance/purch/supplierDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cashPaymentDialog</from-outcome>
            <to-view-id>/finance/purch/cashPaymentDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cashPaymentDeleteConfirmDialog</from-outcome>
            <to-view-id>/finance/purch/cashPaymentDeleteConfirmDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>priceListDialog</from-outcome>
            <to-view-id>/dashboard/finance/priceListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobCostEstimateDialog</from-outcome>
            <to-view-id>/dashboard/finance/jobCostEstimateDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>classificationDialog</from-outcome>
            <to-view-id>/finance/classificationDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobCategoryDialog</from-outcome>
            <to-view-id>/finance/jobCategoryDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>jobSubcategoryDialog</from-outcome>
            <to-view-id>/finance/jobSubcategoryDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>     
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>sectorDialog</from-outcome>
            <to-view-id>/finance/sectorDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>serviceDialog</from-outcome>
            <to-view-id>/finance/serviceDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>procurementMethodDialog</from-outcome>
            <to-view-id>/finance/procurementMethodDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>accountingCodeDialog</from-outcome>
            <to-view-id>/finance/accountingCodeDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>taxDialog</from-outcome>
            <to-view-id>/finance/taxDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>discountDialog</from-outcome>
            <to-view-id>/finance/discountDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>currencyDialog</from-outcome>
            <to-view-id>/finance/currencyDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>marketProductDialog</from-outcome>
            <to-view-id>/finance/marketProductDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventoryProductDialog</from-outcome>
            <to-view-id>/finance/ims/inventoryProductDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
       
    <managed-bean>
        <managed-bean-name>financeManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.FinanceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>purchasingManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.PurchasingManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>accountingManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.AccountingManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>inventoryManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.InventoryManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>tradeManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.tm.manager.TradeManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>humanResourceManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.hrm.manager.HumanResourceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    
</faces-config>
